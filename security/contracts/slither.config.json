{
  "filter_paths": [
    "node_modules",
    "test",
    "mock"
  ],
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "exclude_optimization": false,
  "solc_args": "--optimize --optimize-runs 200",
  "disable_color": false,
  "checkers_to_run": [
    "arbitrary-send-erc20",
    "arbitrary-send-eth", 
    "array-by-reference",
    "assert-state-change",
    "assign-state-variable",
    "boolean-cst",
    "boolean-equal",
    "calls-loop",
    "constable-states",
    "constant-function-asm",
    "constant-function-state",
    "controlled-array-length",
    "controlled-delegatecall",
    "costly-loop",
    "dead-code",
    "delegatecall-loop",
    "deprecated-standards",
    "divide-before-multiply",
    "domain-separator-collision",
    "erc20-interface",
    "erc721-interface",
    "events-access",
    "events-maths",
    "external-function",
    "function-init-state",
    "hardcoded-gas-limit",
    "incorrect-equality",
    "incorrect-modifier",
    "incorrect-shift",
    "incorrect-unary",
    "locked-ether",
    "low-level-calls",
    "mapping-deletion",
    "missing-events-access",
    "missing-events-arithmetic",
    "missing-inheritance",
    "missing-zero-check",
    "multiple-constructors",
    "name-reused",
    "naming-convention",
    "pragma",
    "public-mappings-nested",
    "redundant-statements",
    "reentrancy-benign",
    "reentrancy-eth",
    "reentrancy-events",
    "reentrancy-no-eth",
    "reentrancy-unlimited-gas",
    "return-bomb",
    "return-leave",
    "rtlo",
    "shadowing-abstract",
    "shadowing-builtin",
    "shadowing-local",
    "shadowing-state",
    "similar-names",
    "solc-version",
    "storage-array",
    "suicidal",
    "timestamp",
    "too-many-digits",
    "tx-origin",
    "unchecked-lowlevel",
    "unchecked-send",
    "unchecked-transfer",
    "unimplemented-functions",
    "uninitialized-fptr-cst",
    "uninitialized-local",
    "uninitialized-state",
    "uninitialized-storage",
    "unprotected-upgrade",
    "unused-import",
    "unused-return",
    "unused-state",
    "var-read-using-this",
    "void-cst",
    "weak-prng"
  ],
  "printers_to_run": [
    "call-graph",
    "cfg",
    "constructor-calls",
    "contract-summary",
    "data-dependency",
    "echidna",
    "function-id",
    "function-summary",
    "human-summary",
    "inheritance",
    "inheritance-graph",
    "modifiers",
    "require",
    "slithir",
    "slithir-ssa",
    "variable-order",
    "vars-and-auth"
  ],
  "detectors_to_exclude": [],
  "compilation_unit_format": "standard",
  "legacy_ast": false,
  "generate_patches": true,
  "triage_mode": false,
  "config_file": null,
  "sarif": "security/reports/slither-report.sarif",
  "json": "security/reports/slither-report.json",
  "zip": "security/reports/slither-artifacts.zip"
}
